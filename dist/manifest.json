{
  "manifest_version": 3,
  "name": "chrome-extension-tsx-template",
  "version": "1.0.0",
  "description": "A template for developing Chrome extensions using TSX.",

  "action": {
    "default_title": "chrome-extension-tsx-template",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "32": "icon32.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "permissions": [
    "tabs"
  ],
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
